
public class BattleShip {
	private static String[][] battleMap = new String[5][5];
	
	public static void initMap() {
		for (int i=0; i<5; i++) {
			for (int j=0; j<5; j++) {
				battleMap[i][j] = "â–ˆ";
			}
		}
	}
	
	public static void printMap() {
		for (int i=0; i<5; i++) {
			for (int j=0; j<5; j++) {
				System.out.print(battleMap[i][j]);
			}
			System.out.println();
		}
	}
	
	public static void main(String[] args) {
		initMap();
		printMap();
	}

}
